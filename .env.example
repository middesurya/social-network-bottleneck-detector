# Application Settings
APP_NAME="Social Network Bottleneck Detector"
APP_VERSION="1.0.0"
DEBUG=true
ENVIRONMENT=development

# API Settings
API_V1_PREFIX=/api/v1
CORS_ORIGINS=["http://localhost:5173","http://localhost:3000"]

# Neo4j Database
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=password
NEO4J_DATABASE=neo4j

# Redis Cache
REDIS_URL=redis://localhost:6379/0
CACHE_TTL=3600

# LLM / Natural Language Queries (Optional)
OPENAI_API_KEY=your-openai-api-key-here
LLM_MODEL=gpt-4
LLM_TEMPERATURE=0.0

# Algorithm Settings
DEFAULT_PAGE_RANK_ITERATIONS=20
DEFAULT_PAGE_RANK_DAMPING=0.85
